@{
}
@{
    ViewData["Title"] = "Recursos humanos";
}

@foreach (var recurso in (IEnumerable<dynamic>) ViewData["recursos"])
{
    <div class="card" style="width: 18rem;">
        <img src="@recurso.media" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@recurso.titulo</h5>
            <p class="card-text">@recurso.descripcion</p>
        </div>
    </div>
}

